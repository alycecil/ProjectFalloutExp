    <ThingDef ParentName="MealBase">
        <defName>MealPFDandyBoyApples</defName>
        <label>Dandy Boy Apples</label>
        <description>"Apples so good, they never go bad!". An old package of freeze dried apples, they taste a bit
            stale, but they are still nutritious.
        </description>
        <graphicData>
            <texPath>Things/Fallout/Food/Pre-war/DandyApplesItem</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <stackLimit>25</stackLimit>
        <statBases>
            <DeteriorationRate>0.25</DeteriorationRate>
            <MarketValue>11</MarketValue>
            <Mass>0.23</Mass>
            <Nutrition>0.75</Nutrition>
        </statBases>
        <ingestible>
            <preferability>MealSimple</preferability>
            <foodType>Processed</foodType>
            <ingestSound>Meal_Eat</ingestSound>
            <ingestEffect>EatVegetarian</ingestEffect>
            <ingestCommandString>Eat {0}</ingestCommandString>
            <ingestReportString>Eating {0}.</ingestReportString>
        </ingestible>
    </ThingDef>

