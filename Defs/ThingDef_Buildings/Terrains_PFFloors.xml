<?xml version="1.0" encoding="UTF-8"?>

<Defs>

    <TerrainDef ParentName="FloorBase">
        <defName>PFVaultFloor</defName>
        <label>Vault Floor Tiles</label>
        <renderPrecedence>300</renderPrecedence>
        <description>A thick metal floor built for keeping out ground radiation. It is fairly pleasing to the eyes, and
            can be cleaned with ease</description>
        <texturePath>Things/Fallout/Buildings/Floors/VaultFloor</texturePath>
        <statBases>
            <Beauty>1</Beauty>
            <Cleanliness>0.3</Cleanliness>
            <WorkToBuild>900</WorkToBuild>
        </statBases>
        <costList>
            <Steel>1</Steel>
            <Plasteel>1</Plasteel>
        </costList>
        <constructEffect>ConstructMetal</constructEffect>
    </TerrainDef>


</Defs>